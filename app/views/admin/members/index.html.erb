<div class="row">
	<div class="col-sm-12">
		<div class="sub-header">
			<h1 class="pull-left">Members</h1>
			<div class="pull-right align-button">
				<%= link_to 'New Member', new_admin_member_path, class: 'btn btn-success' %>
			</div>
		</div>

		<table class="table table-bordered">
			<thead>
				<th>Name</th>
				<th>Email</th>
				<th colspan="4" width="50%">Actions</th>
			</thead>
			<tbody>
				<% @members.each do |member|  %>
					<tr>
						<td><%= member.name %></td>
						<td><%= member.email %></td>
						<td width="11%">Check in</td>
						<td width="11%">Check out</td>
						<td width="11%"><%= link_to 'Show Member', [member] %></td>
						<td width="11%"><%= link_to 'Edit Member', edit_admin_member_path(member) %></td>
					</tr>
				<% end %>
			</tbody>
		</table>
	</div>
</div>